{
	"name": "fluid-md-editor",
	"type": "module",
	"version": "0.8.1",
	"description": "A standalone Markdown editor that can render notes to PDF with differents themes, build with Astro, shine with Pico",
	"scripts": {
		"dev": "bunx --bun astro dev",
		"start": "bunx --bun astro dev",
		"prepare": "bunx --bun panda codegen --clean",
		"build:astro": "bunx --bun astro check && astro build",
		"preview": "bunx --bun astro preview",
		"astro": "bunx --bun astro",
		"format": "bunx --bun prettier . --write"
	},
	"keywords": [
		"astro",
		"pico"
	],
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/solid-js": "^5.1.0",
		"astro": "^5.10.1",
		"easymde": "^2.20.0",
		"marked": "^15.0.12",
		"marked-katex-extension": "^5.1.5",
		"solid-js": "^1.9.7",
		"typescript": "^5.8.3",
		"vite-plugin-dynamic-import": "^1.6.0"
	},
	"devDependencies": {
		"@pandacss/dev": "^0.52.0",
		"@types/bun": "^1.2.17",
		"pandacss-preset-typography": "^0.1.6",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"sass-embedded": "^1.89.2",
		"scss": "^0.2.4"
	}
}
